{
  "name": "@auraui/styles",
  "version": "0.0.0",
  "description": "auraui styles package",
  "keywords": [
    "vue",
    "ui",
    "component library"
  ],
  "author": "leewahjoel",
  "license": "MIT",
  "homepage": "https://github.com/79rose/aura-ui/master/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/79rose/aura-ui.git"
  },
  "bugs": {
    "url": "https://github.com/79rose/aura-ui/issues"
  },
  "scripts": {
    "build:theme": "vite build --mode theme",
    "build:unocss": "vite build --mode unocss",
    "build": "pnpm run build:unocss && pnpm run build:theme"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/auraui-styles.umd.js",
  "module": "./dist/auraui-styles.mjs",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/auraui-styles.umd.js",
      "import": "./dist/auraui-styles.mjs",
      "types": "./dist/src/index.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./preset": {
      "require": "./dist/preset.umd.js",
      "import": "./dist/preset.mjs",
      "types": "./dist/src/unoPreset.d.ts"
    },
    "./*": "./*"
  },
  "peerDependencies": {
    "vue": ">=3.0.0",
    "unocss": ">=0.54.1"
  },
  "dependencies": {
    "@auraui/shared": "workspace:^"
  }
}
